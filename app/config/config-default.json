{
  "$schema": "./schema.json",
  "config": {
    "updateChannel": "stable",
    "fontSize": 14,
    "fontFamily": "JetBrains Mono, JetBrainsMono, Fira Code, Menlo, \"DejaVu Sans Mono\", Consolas, monospace",
    "fontWeight": "400",
    "fontWeightBold": "600",
    "lineHeight": 1.4,
    "letterSpacing": 0.5,
    "scrollback": 1000,
    "cursorColor": "rgba(139,233,253,0.9)",
    "cursorAccentColor": "#1a1b26",
    "cursorShape": "BEAM",
    "cursorBlink": true,
    "foregroundColor": "#c0caf5",
    "backgroundColor": "rgba(26, 27, 38, 0.95)",
    "selectionColor": "rgba(139,233,253,0.2)",
    "borderColor": "rgba(86,95,137,0.3)",
    "css": "",
    "termCSS": "",
    "workingDirectory": "",
    "showHamburgerMenu": "",
    "showWindowControls": "",
    "padding": "20px 24px",
    "colors": {
      "black": "#15161e",
      "red": "#f7768e",
      "green": "#9ece6a",
      "yellow": "#e0af68",
      "blue": "#7aa2f7",
      "magenta": "#bb9af7",
      "cyan": "#7dcfff",
      "white": "#a9b1d6",
      "lightBlack": "#414868",
      "lightRed": "#f7768e",
      "lightGreen": "#9ece6a",
      "lightYellow": "#e0af68",
      "lightBlue": "#7aa2f7",
      "lightMagenta": "#bb9af7",
      "lightCyan": "#7dcfff",
      "lightWhite": "#c0caf5",
      "limeGreen": "#9ece6a",
      "lightCoral": "#f7768e"
    },
    "shell": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
    "shellArgs": [
      "-NoLogo"
    ],
    "env": {},
    "bell": "SOUND",
    "bellSound": null,
    "bellSoundURL": null,
    "copyOnSelect": false,
    "defaultSSHApp": true,
    "quickEdit": false,
    "macOptionSelectionMode": "vertical",
    "webGLRenderer": false,
    "webLinksActivationKey": "",
    "disableLigatures": false,
    "disableAutoUpdates": true,
    "autoUpdatePlugins": true,
    "preserveCWD": true,
    "screenReaderMode": false,
    "imageSupport": true,
    "defaultProfile": "default",
    "profiles": [
      {
        "name": "default",
        "config": {}
      }
    ]
  },
  "plugins": [],
  "localPlugins": [],
  "keymaps": {}
}
